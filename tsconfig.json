{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ES5",
    "lib": ["DOM", "DOM.Iterable", "ES2015"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist/",
    "checkJs": false,
    "strictNullChecks": false,
    "jsx": "react-jsx",
    "downlevelIteration": true,
	// "strict": true,
	"noImplicitAny": false
  },
  "include": [
    "src"
  ]
}
